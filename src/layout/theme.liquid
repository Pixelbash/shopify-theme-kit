{% include 'liquid-globals' %}
<!doctype html>
<html dir="ltr" lang="en" class="no-js">
  {% include 'common-head-meta' %}
  {% include 'common-head-styles' %}
  {% include 'common-head-icons' %}
  {% include 'common-head-scripts' %}
  {% include 'common-head-extra' %}
  {{ content_for_header }}
  {% include 'common-head-analytics' %}
</head>

<body class="{{ template_class }}" id="{{ page_title | handle }}">
  {% if template contains == 'index' %}
    {% include 'element-landing' %}
  {% endif %}

  {% include 'common-header' %}

  <section class="container-wrap container-wrap-{{ template_class }}">
    {{ content_for_layout }}
  </section>

  {% include 'common-footer' %}
</body>
</html>
 