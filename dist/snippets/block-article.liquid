<div class="block">
  <div class="image">
    <img src="{{ article.metafields.article.featured_image }}" alt="{{ article.title }}">
  </div>
  <div class="description">
    <div class="title">
      <h2>{{ article.title }}</h2>
    </div>
    <div class="content no-symbol">
      {{ article.content }}
    </div>
  </div>
</div>